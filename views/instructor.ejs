<!DOCTYPE html>
<html >
  <head>
    <meta charset="UTF-8">
    <title>students</title>
  </head>

  <body>
    <li><a href="/create_new_work">Create New Work</a></li>
    <li><a href="/students">Show all students</a></li>
    <section class="ac-container">
      <form action="instructor/drop_one_submission" method="post">
      <% rules.forEach( function(item, index) { 
        var btn_name = "btn_drop_" + index;
        var value_name = "Drop " + item.work_name; %>
        <label><%= item.work_name + ' ' + item.num_peers + ' ' + item.required_files %></label>
        <input type="submit" name="<%= btn_name %>" value="<%= value_name %>"></input>
        <br />

      <% }); %>
      </form>
    </section>
  </body>
</html>
